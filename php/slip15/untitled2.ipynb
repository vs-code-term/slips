      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Lists\n",
        "subjects = ['Math', 'Physics', 'Chemistry', 'Biology', 'English']\n",
        "marks = [85, 78, 90, 75, 80]\n",
        "\n",
        "# Pie chart\n",
        "plt.pie(marks, labels=subjects, autopct='%1.1f%%')\n",
        "plt.title('Marks Distribution')\n",
        "plt.show()"
      ]
    }
  ],
