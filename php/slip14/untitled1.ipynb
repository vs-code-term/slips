      "source": [
        "import numpy as np\n",
        "\n",
        "arr = np.array([[3, 7, 1], [9, 2, 5]])\n",
        "\n",
        "flat_arr = arr.flatten()\n",
        "\n",
        "weights = np.array([0.1, 0.2, 0.1, 0.2, 0.2, 0.2])\n",
        "\n",
        "weighted_avg = np.average(flat_arr, weights=weights)\n",
        "\n",
        "print(\"Weighted average:\", weighted_avg)"
      ]
    }
