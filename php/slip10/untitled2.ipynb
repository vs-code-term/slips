      "source": [
        "import itertools\n",
        "\n",
        "points = [(1, 2), (3, 4), (5, 0), (2, 1)]\n",
        "\n",
        "total_distance = 0\n",
        "\n",
        "for (x1, y1), (x2, y2) in itertools.combinations(points, 2):\n",
        "    total_distance += abs(x1 - x2) + abs(y1 - y2)\n",
        "\n",
        "print(\"Sum of Manhattan distances between all pairs:\", total_distance)"
      ]
    }
