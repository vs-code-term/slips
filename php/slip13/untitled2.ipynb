      "source": [
        "import numpy as np\n",
        "\n",
        "arr = np.array([[3, 7, 1], [9, 2, 5]])\n",
        "\n",
        "flat_arr = arr.flatten()\n",
        "\n",
        "max_val = flat_arr.max()\n",
        "min_val = flat_arr.min()\n",
        "\n",
        "print(\"Maximum value:\", max_val)\n",
        "print(\"Minimum value:\", min_val)"
      ]
    }
